msgid "Torrent cannot be enqueued"
msgstr "无法将种子排入队列"

#: ktorrent/ktorrent.notifyrc:463
msgctxt "Name"
msgid "Torrent cannot be started"
msgstr "无法启动种子"

#: ktorrent/ktorrent.notifyrc:515
msgctxt "Name"
msgid "Torrent cannot be loaded silently"
msgstr "无法静默地加载种子"

#: ktorrent/ktorrent.notifyrc:567
msgctxt "Name"
msgid "DHT is not enabled"
msgstr "未启用 DHT"

#: ktorrent/ktorrent.notifyrc:614
msgctxt "Name"
msgid "Event generated by plugin"
msgstr "插件产生的事件"

#: ktorrent/ktorrent.notifyrc:658
msgctxt "Name"
msgid "Magnet link download started"
msgstr "已开始磁力链下载"

#: ktorrent/ktorrentplugin.desktop:4
msgctxt "Name"
msgid "KTorrent Plugin"
msgstr "KTorrent 插件"

#: plasma/applet/plasma-applet-ktorrent.desktop:61
msgctxt "Comment"
msgid "Plasma widget to keep track of a single torrent"
msgstr "跟踪单个种子的 Plasma 部件"

#: plasma/dataengine/plasma-dataengine-ktorrent.desktop:59
msgctxt "Comment"
msgid "KTorrent data engine, for getting information from KTorrent"
msgstr "KTorrent 数据引擎，用于从 KTorrent 获取信息"

#: plugins/bwscheduler/ktbwschedulerplugin.desktop:2
msgctxt "Name"
msgid "Scheduler"
msgstr "调度器"

#: plugins/bwscheduler/ktbwschedulerplugin.desktop:45
msgctxt "Comment"
msgid "Schedule upload and download limits over a period of a week"
msgstr "调度一周内的上传和下载限制"

#: plugins/downloadorder/ktdownloadorderplugin.desktop:2
msgctxt "Name"
msgid "Download Order"
msgstr "下载顺序"

#: plugins/downloadorder/ktdownloadorderplugin.desktop:51
msgctxt "Comment"
msgid "Specify the download order of a multi-file torrent"
msgstr "指定多文件种子的下载顺序"

#: plugins/infowidget/ktinfowidgetplugin.desktop:2
msgctxt "Name"
msgid "Information Widget"
msgstr "信息部件"

#: plugins/infowidget/ktinfowidgetplugin.desktop:50
msgctxt "Comment"
msgid "Displays general information about a torrent in several tabs"
msgstr "在若干标签内显示一个种子的常规信息"

#: plugins/ipfilter/ktipfilterplugin.desktop:2
msgctxt "Name"
msgid "IP Filter"
msgstr "IP 过滤器"

#: plugins/ipfilter/ktipfilterplugin.desktop:52
msgctxt "Comment"
msgid "Filter IP addresses through a blocklist"
msgstr "通过屏蔽列表过滤 IP 地址"

#: plugins/logviewer/ktlogviewerplugin.desktop:2
msgctxt "Name"
msgid "Log Viewer"
msgstr "日志查看器"

#: plugins/logviewer/ktlogviewerplugin.desktop:53
msgctxt "Comment"
msgid "Displays the logging output"
msgstr "显示日志输出"

#: plugins/magnetgenerator/ktmagnetgeneratorplugin.desktop:2
msgctxt "Name"
msgid "Magnet Generator"
msgstr "磁力链生成器"

#: plugins/magnetgenerator/ktmagnetgeneratorplugin.desktop:45
msgctxt "Comment"
msgid "Generates magnet links"
msgstr "生成磁力链"

#: plugins/mediaplayer/ktmediaplayerplugin.desktop:2
msgctxt "Name"
msgid "Media Player"
msgstr "媒体播放器"

#: plugins/mediaplayer/ktmediaplayerplugin.desktop:53
msgctxt "Comment"
msgid "Phonon-based media player"
msgstr "基于 Phonon 的媒体播放器"

#: plugins/scanfolder/ktscanfolderplugin.desktop:2
msgctxt "Name"
msgid "Scan Folder"
msgstr "扫描文件夹"

#: plugins/scanfolder/ktscanfolderplugin.desktop:52
msgctxt "Comment"
msgid "Scan folders for torrent files and load them"
msgstr "扫描文件夹以装入其中的种子文件"

#: plugins/scripting/ktscriptingplugin.desktop:2
msgctxt "Name"
msgid "Scripting"
msgstr "脚本"

#: plugins/scripting/ktscriptingplugin.desktop:51
msgctxt "Comment"
msgid "Enables Kross scripting support"
msgstr "启用 Kross 脚本支持"

#: plugins/scripting/scripts/auto_remove/auto_remove.desktop:2
msgctxt "Name"
msgid "Automatic Remove"
msgstr "自动移除"

#: plugins/scripting/scripts/auto_remove/auto_remove.desktop:51
msgctxt "Comment"
msgid ""
"Script to automatically remove torrents when they finish downloading or "
"seeding"
msgstr "下载完成或出种时自动移除种子的脚本"

#: plugins/scripting/scripts/auto_resume/auto_resume.desktop:2
msgctxt "Name"
msgid "Automatic Resume"
msgstr "自动恢复"

#: plugins/scripting/scripts/auto_resume/auto_resume.desktop:52
msgctxt "Comment"
msgid ""
"Script to automatically resume after some time, when the suspended state is "
"enabled"
msgstr "当启用暂停状态时，自动在某段时间后恢复下载的脚本"

#: plugins/scripting/scripts/email_notifications/email_notifications.desktop:2
msgctxt "Name"
msgid "E-Mail Notifications"
msgstr "电子邮件通知"

#: plugins/scripting/scripts/email_notifications/email_notifications.desktop:52
msgctxt "Comment"
msgid "Script to notify you of events via e-mail"
msgstr "将事件通过电子邮件通知您的脚本"

#: plugins/scripting/scripts/tracker_groups/tracker_groups.desktop:2
msgctxt "Name"
msgid "Tracker Grouping"
msgstr "追踪服务器分组"

#: plugins/scripting/scripts/tracker_groups/tracker_groups.desktop:51
msgctxt "Comment"
msgid "Script to create automatic groups based upon tracker URLs"
msgstr "基于追踪服务器网址创建自动分组的脚本"

#: plugins/search/ktsearchplugin.desktop:2
msgctxt "Name"
msgid "Search"
msgstr "搜索"

#: plugins/search/ktsearchplugin.desktop:53
msgctxt "Comment"
msgid "Search for torrents"
msgstr "搜索种子"

#: plugins/shutdown/ktshutdownplugin.desktop:2
msgctxt "Name"
msgid "Shutdown"
msgstr "关机"

#: plugins/shutdown/ktshutdownplugin.desktop:53
msgctxt "Comment"
msgid "Allows you to shutdown your computer when torrents finish"
msgstr "允许您在种子完成时关闭计算机"

#: plugins/stats/ktstatsplugin.desktop:3
msgctxt "Name"
msgid "Statistics"
msgstr "统计"

#: plugins/stats/ktstatsplugin.desktop:55
msgctxt "Comment"
msgid "Shows statistics about torrents in several graphs"
msgstr "以图表形式显示种子的统计"

#: plugins/syndication/ktsyndicationplugin.desktop:2
msgctxt "Name"
msgid "Syndication"
msgstr "同步"

#: plugins/syndication/ktsyndicationplugin.desktop:50
msgctxt "Comment"
msgid "Syndication plugin for KTorrent, supporting RSS and Atom feeds"
msgstr "KTorrent 的同步插件，支持 RSS 和 Atom 订阅"

#: plugins/upnp/ktupnpplugin.desktop:2
msgctxt "Name"
msgid "UPnP"
msgstr "UPnP"

#: plugins/upnp/ktupnpplugin.desktop:54
msgctxt "Comment"
msgid "Forward ports using UPnP"
msgstr "使用 UPnP 转发端口"

#: plugins/webinterface/ktwebinterfaceplugin.desktop:2
msgctxt "Name"
msgid "Web Interface"
msgstr "网页界面"

#: plugins/webinterface/ktwebinterfaceplugin.desktop:53
msgctxt "Comment"
msgid "Allows control of KTorrent via a web interface"
msgstr "允许通过网页界面控制 KTorrent"

#: plugins/zeroconf/ktzeroconfplugin.desktop:2
msgctxt "Name"
msgid "Zeroconf"
msgstr "ZeroConf"

#: plugins/zeroconf/ktzeroconfplugin.desktop:52
msgctxt "Comment"
msgid "Discover peers on the local network using the Zeroconf protocol"
msgstr "通过 zeroconf 协议在本地网络探索对等点"

