#: ktorrent/ktorrent.desktop:123
msgctxt "Comment"
msgid "A BitTorrent program for KDE"
msgstr "一个 KDE 的 BitTorrent 程序"

#: ktorrent/ktorrent.notifyrc:3
msgctxt "Comment"
msgid "KTorrent"
msgstr "KTorrent"

#: ktorrent/ktorrent.notifyrc:59
msgctxt "Name"
msgid "Torrent silently opened"
msgstr "种子已静默打开"

#: ktorrent/ktorrent.notifyrc:98
