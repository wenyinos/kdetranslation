#: plugins/paintops/hairy/wdghairyeoptions.ui:310
#: plugins/paintops/hairy/wdgInkOptions.ui:68
#: plugins/paintops/libpaintop/kis_pressure_hsv_option.cpp:30
#: ui/forms/wdgapplyprofile.ui:116 ui/forms/wdgcolorsettings.ui:140
